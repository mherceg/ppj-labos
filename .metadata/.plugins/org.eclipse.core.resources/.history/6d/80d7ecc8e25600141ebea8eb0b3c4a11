import java.util.ArrayList;
import java.util.List;


public class Definicija {
	private List<Stanje> stanje;

	public List<Stanje> getStanje() {
		if (stanje == null){
			stanje = new ArrayList<>();
		}
		return stanje;
	}

	public void setStanje(List<Stanje> stanje) {
		this.stanje = stanje;
	}
	
}
