%V <A> <B>
%T a b
%Syn b
<A>
 <B> <A>
<B>
 a <B>
 b
<A>
 $
 
%V <A> <B> <C> <D>
%T a b d e
%Syn b
<A>
 <B> <A>
<B>
 a <B>
 b
 $
<A>
 $
<C>
 <A> d
<D>
 <A> <B> <C> e
 
%V <A> <B> <C> <D> <E>
%T a b c d e f
%Syn b
<A>
 <B> <C> c
 e <D> <B>
<B>
 $
 b <C> <D> <E>
<C>
 <D> a <B>
 c a
<D>
 $
 d <D>
<E>
